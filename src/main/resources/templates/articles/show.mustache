{{>layouts/header}}
<table class="table table-striped">
    <thead>
    <tr>
        <th scope="col">id</th>
        <th scope="col">title</th>
        <th scope="col">content</th>
    </tr>
    </thead>
    <tbody>
{{#article}}
    <tr>
        <th scope="row">{{id}}</th>
        <td>{{title}}</td>
        <td>{{content}}</td>
    </tr>
{{/article}}
    </tbody>
</table>
<a href="/articles/new">게시글 작성</a>
<a href="/articles">목록</a>
<a href="/articles/{{id}}/edit" class="btn btn-Primary">Edit</a>
<a href="/articles/{{id}}/delete" class="btn btn-danger">Delete</a>
{{>layouts/footer}}
